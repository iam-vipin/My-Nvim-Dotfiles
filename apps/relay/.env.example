# Server Configuration
PORT=3300
NODE_ENV=development

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Channel Configuration
CHANNEL_PREFIX=relay

# Relay Base Path (for proxy deployment)
RELAY_BASE_PATH=/relay

# API Base URL for authentication (Django API)
API_BASE_URL=http://localhost:8000

# CORS origin for socket connections (web app URL)
CORS_ORIGIN=http://localhost:3000

# AMQP Configuration (for consumer)
AMQP_URL=amqp://localhost:5672
EVENT_STREAM_EXCHANGE=plane.event_stream
PREFETCH_COUNT=10
