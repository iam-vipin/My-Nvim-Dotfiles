    "check:lint": "eslint . --max-warnings 180",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{ts,tsx,md,json,css,scss}\"",
    "fix:lint": "eslint . --fix",
    "fix:format": "prettier --write \"**/*.{ts,tsx,md,json,css,scss}\""
  },
  "dependencies": {
    "@flatfile/api": "^1.12.0",
