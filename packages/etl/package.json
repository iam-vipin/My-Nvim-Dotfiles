    "./slack": "./dist/slack/index.js",
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf .turbo && rm -rf .next && rm -rf node_modules && rm -rf dist",
    "check:lint": "eslint . --cache --cache-location node_modules/.cache/eslint/ --max-warnings=614",
