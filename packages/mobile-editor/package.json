{
  "name": "@plane/mobile-editor",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "lint-mobile": "eslint .",
    "preview": "vite preview",
    "clean": "rm -rf .turbo && rm -rf .next && rm -rf node_modules && rm -rf dist",
    "check:lint-mobile": "eslint . --cache --cache-location node_modules/.cache/eslint/ --max-warnings=28",
    "check:types-mobile": "tsc --noEmit",
    "check:format-mobile": "prettier . --cache --check",
    "fix:lint-mobile": "eslint . --cache --cache-location node_modules/.cache/eslint/ --fix --max-warnings=28",
    "fix:format-mobile": "prettier . --cache --write"
  },
  "dependencies": {
    "@plane/constants": "workspace:*",
    "@plane/editor": "workspace:*",
    "@plane/ui": "workspace:*",
    "@plane/utils": "workspace:*",
    "@plane/propel": "workspace:*",
    "lucide-react": "^0.454.0",
    "lodash": "^4.17.23",
    "mobx": "^6.10.0",
    "mobx-react": "^9.1.1",
    "mobx-utils": "^6.0.8",
    "next-themes": "0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@plane/tailwind-config": "workspace:*",
    "@plane/types": "workspace:*",
    "@tailwindcss/typography": "0.5.19",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "vite": "catalog:",
    "vite-plugin-node-polyfills": "^0.22.0"
  }
}
