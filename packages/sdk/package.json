{
  "name": "@plane/sdk",
  "version": "0.23.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "clean": "rm -rf .turbo && rm -rf .next && rm -rf node_modules && rm -rf dist",
    "check:lint": "eslint . --cache --cache-location node_modules/.cache/eslint/ --max-warnings=272",
    "check:types": "tsc --noEmit",
    "check:format": "prettier . --cache --check",
    "fix:lint": "eslint . --cache --cache-location node_modules/.cache/eslint/ --max-warnings=272 --fix",
    "fix:format": "prettier . --cache --write"
  },
  "dependencies": {
    "axios": "catalog:"
  },
  "devDependencies": {
    "@plane/types": "workspace:*",
    "@plane/typescript-config": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
